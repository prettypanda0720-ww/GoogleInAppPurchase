<?xml version='1.0' encoding='utf-8'?>
<widget 
        xmlns="http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id="com.intuitivechessapp3.app" 
        version="1.6.5" 
        xmlns:cdv="http://cordova.apache.org/ns/1.0">

    <name>PurchaseNC</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>

    <author href="http://intuitivechessapp.com" email="sergio.tadela@gmail.com">
        Intuitive Chess Team
    </author>



    <content src="index.html" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="android-targetSdkVersion" value="28" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cc.fovea.cordova.purchase" spec="^7.4.1">
        <variable name="BILLING_KEY" value="" />
    </plugin>
    <!--<engine name="android" spec="^7.1.4" />-->
</widget>
